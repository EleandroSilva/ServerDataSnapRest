unit Model.Entidade.Pessoa;

interface

uses
  Model.Entidade.Pessoa.Interfaces,
  System.JSON,
  Controller.Conexao.Factory.Interfaces;

type
  TPessoa = class(TInterfacedObject, iEntidadePessoa)
    private
      FController : iControllerConexaoFactory;
    public
      constructor Create;
      destructor Destroy; override;
      class function New : iEntidadePessoa;

      function Get(const Key: String): TJsonArray;
  end;

implementation

uses
  Controller.Conexao.Factory;

{ TPessoa }

constructor TPessoa.Create;
begin
  FController := TControllerConexaoFactory.New;
end;

destructor TPessoa.Destroy;
begin
  inherited;
end;

class function TPessoa.New: iEntidadePessoa;
begin
  Result := Self.Create;
end;

function TPessoa.Get(const Key: String): TJsonArray;
begin

end;

end.
